.fdb-touch
  position relative
  z-index 2
  transition all .5s ease-in-out
  margin-top 40px
  img
    position absolute
    height 100%
    top 0
    left 0
    z-index -2
    opacity .3
  &:after
    content ""
    position absolute
    height 100%
    width 100%
    background linear-gradient(to bottom, rgba(0,0,0,0) 0%,rgba(255,255,255,0.65) 100%)
    top 0
    left 0
    z-index 0
  &:hover
    transform scale(1.05)
  p
    z-index 2
    color #222
  a
    z-index 2
    position relative
  h2
    z-index 2
.footer-large
  ul
    list-style none
.loggedIn
  width 100%
  height auto
  min-height 40px
  background #329ef7
  a
    color white